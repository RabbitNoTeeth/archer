<idea-plugin>
    <id>fun.bookish.plugin.archer.avengers</id>
    <name>avengers代码生成器</name>
    <vendor email="864426487@qq.com" url="http://blog.bookish.fun">Liuxindong</vendor>

    <description>avengers代码生成器</description>

