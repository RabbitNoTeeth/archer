<idea-plugin>
    <id>fun.bookish.plugin.archer.aigner.mysql.v1</id>
    <name>Archer for aigner</name>
    <vendor email="864426487@qq.com" url="http://blog.bookish.fun">Liuxindong</vendor>

    <description>aigner框架代码生成器</description>

